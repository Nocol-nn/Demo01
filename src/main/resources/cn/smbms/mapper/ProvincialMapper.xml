<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.smbms.mapper.ProvincialMapper">

    <select id="getAllProvincial" resultType="Provincial">
        select * from smbms_provincial
    </select>
    <select id="getProvincialCodeById" parameterType="int" resultType="Provincial">
        select * from smbms_provincial where id=#{id}
    </select>
</mapper>